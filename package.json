{
  "name": "Coq",
  "description": "Coq Support for Visual Studio Code",
  "version": "0.1.0",
  "publisher": "zjhmale",
  "engines": {
    "vscode": "^0.10.1"
  },
  "icon": "images/coq.png",
  "keywords": [
    "language",
    "coq",
    "highlight",
    "syntax",
    "snippets"
  ],
  "license": "BSD",
  "homepage": "https://github.com/zjhmale/vscode-coq",
  "repository": {
    "type": "git",
    "url": "https://github.com/zjhmale/vscode-coq.git"
  },
  "bugs": {
    "url": "https://github.com/zjhmale/vscode-coq/issues",
    "email": "zjhmale@gmail.com"
  },
  "categories": [
    "Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:coq"
  ],
  "main": "./src/extension",
  "contributes": {
    "languages": [{
      "id": "coq",
      "aliases": [
        "Coq",
        "coq"
      ],
      "extensions": [
        ".v"
      ],
      "configuration": "./configs/coq.json"
    }],
    "grammars": [{
      "language": "coq",
      "scopeName": "source.coq",
      "path": "./syntaxes/coq.tmLanguage"
    }],
    "commands": [{
      "command": "coq.next",
      "title": "Coq: next"
    }, {
      "command": "coq.prev",
      "title": "Coq: prev"
    }, {
      "command": "coq.to-cursor",
      "title": "Coq: to cursor"
    }],
    "keybindings": [{
      "key": "shift+ctrl+n",
      "mac": "shift+cmd+n",
      "command": "coq.next"
    }, {
      "key": "shift+ctrl+u",
      "mac": "shift+cmd+u",
      "command": "coq.prev"
    }, {
      "key": "shift+ctrl+enter",
      "mac": "shift+cmd+enter",
      "command": "coq.to-cursor"
    }],
    "snippets": [{
      "language": "coq",
      "path": "./snippets/coq.json"
    }]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "xml2json": "0.9.0",
    "rx-lite": "4.0.0",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "vscode": "^0.11.0"
  }
}
